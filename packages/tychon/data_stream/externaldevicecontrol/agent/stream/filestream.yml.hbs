paths:
{{#each paths as |path|}}
  - {{path}}
{{/each}}
prospector.scanner.exclude_files: ['.gz$']
publisher_pipeline.disable_host: true
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#if disable_cdrom}}
  - disable_cdrom={{disable_cdrom}}
{{/if}}
{{#if disable_drive}}
  - disable_drive={{disable_drive}}
{{/if}}
{{#if idlist}}
{{#each idlist as |iditem|}}
  - hwid={{iditem}}
{{/each}}
{{/if}}
{{#if processors}}
processors:
{{processors}}
{{/if}}
